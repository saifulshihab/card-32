{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": [0],
    "quotes": "off",
    "no-shadow": "off",
    "import/extensions": [0],
    "import/prefer-default-export": "off",
    "comma-dangle": [0],
    "implicit-arrow-linebreak": [0]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts"]
      }
    }
  }
}
